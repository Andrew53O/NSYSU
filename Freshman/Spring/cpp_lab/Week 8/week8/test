Q1=q1
Q2=q2
Q3=q3
Q4=q4
clear
make
if [ "$Q1"!="N" ]; then
	echo "----------Q1----------"
	./$Q1 <<< "2022 12 6 
		2022 3 18"
	echo "----------"
	./$Q1 <<< "2023 4 8
		2023 4 8"
fi
if [ "$Q2"!="N" ]; then
	echo "----------Q2----------"
	./$Q2 <<< "60 23 94 65 20 80 75 66 23 4 98 99 65 32 46 89 56 23 2 44 20 53 -1"
	echo "----------"
	./$Q2 <<< "60 23 94 65 20 80 100 75 66 23 4 98 99 65 32 46 89 56 23 2 44 20 53 -1"
fi
if [ "$Q3"!="N" ]; then
	echo "----------Q3----------"
	./$Q3 <<< "Jeff 1994 12 6
		Claire 2020 7 15
		Alex 1998 6 19 
		Tina 2001 11 10 
		Iris 2002 1 17 
		Bob 1994 12 5
		exit"
fi
if [ "$Q4"!="N" ]; then
	echo "----------Q4----------"
	./$Q4 <<< "1 2012 4 3 600
		1 2001 3 12 9000
		1 2023 4 3 5000
		1 2023 4 3 -1000
		1 2023 4 3 -500
		1 2012 4 3 -20000
		2
		3 2023 4 3
		3 2012 4 3
		4"
fi



