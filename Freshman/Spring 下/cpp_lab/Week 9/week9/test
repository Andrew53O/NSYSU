Q1=q1
Q2=q2
Q3=q3
Q4=q4
clear
make
if [ "$Q1"!="N" ]; then
	echo "----------Q1----------"
	./$Q1 <<< "2023 01 01 
		2023 4 20
		2023 4 20
		date2 date3"
	echo "----------"
	./$Q1 <<< "2023 01 01 
		2022 4 20
		2022 12 25
		date1 date3"
fi
if [ "$Q2"!="N" ]; then
	echo "----------Q2----------"
	./$Q2 <<< "2022 4 20 
		2022 1 1
		2028 11 3
		date1
		15"
fi
if [ "$Q3"!="N" ]; then
	echo "----------Q3----------"
	./$Q3 <<< "2023 4 20 
		2023 6 30"
	echo "----------"
	./$Q3 <<< "2022 12 25 
		2023 4 20"
fi
if [ "$Q4"!="N" ]; then
	echo "----------Q4----------"
	./$Q4 <<< "2
	3
	5
	6"
	echo "----------"
	./$Q4 <<< "2
	5
	4
	10"
fi



